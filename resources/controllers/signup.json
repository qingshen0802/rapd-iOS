{
  "background_color": "ffffff",
  "title_color": "364655",
  "form_controller": true,
  "elements": [
    {
      "outlet": "profile_image",
      "type": "image",
      "name": "images/profile_placeholder.png",
      "corner_radius": 35,
      "autolayout": {
        "width": 70,
        "height": 70,
        "top": 70,
        "center_x": -70
      }
    },
    
    {
      "outlet": "profile_image_button",
      "type": "button",
      "title": "Mudar Imagem",
      "corner_radius": 3.0,
      "action": "selectPhoto",
      "color": "F15E4F",
      "autolayout": {
        "top": 90,
        "center_x": 40,
        "width": 120,
        "height": 26
      },
      "font": {
        "color": "FFFFFF",
        "size": 12,
        "name": "Roboto-Bold"
      }
    },
    
    {
      "outlet": "username_field",
      "next_outlet": "name_field",
      "type": "text_field",
      "auto_capitalization": false,
      "scroll_top": 0,
      "placeholder": "Defina seu nome de usu√°rio",
      "underlined": true,          
      "autolayout": {
        "width": 300,
        "center_x": 0,
        "top": 150,
        "height": 40
      }
    },
    
    {
      "outlet": "name_field",
      "next_outlet": "email_field",          
      "type": "text_field",
      "auto_capitalization": true,
      "scroll_top": 0,
      "placeholder": "Nome completo",
      "underlined": true,          
      "autolayout": {
        "width": 300,
        "center_x": 0,
        "top": 200,
        "height": 40
      }
    },
    
    {
      "outlet": "email_field",
      "next_outlet": "document_id_field",          
      "type": "text_field",
      "auto_capitalization": false,
      "scroll_top": 0,
      "placeholder": "Email",
      "underlined": true,          
      "autolayout": {
        "width": 300,
        "center_x": 0,
        "top": 250,
        "height": 40
      }
    },
    
    {
      "outlet": "document_id_field",
      "next_outlet": "password_field",
      "type": "text_field",
      "number_only": true,
      "mask": true,
      "auto_capitalization": false,
      "scroll_top": 0,
      "placeholder": "Seu CPF",
      "underlined": true,          
      "autolayout": {
        "width": 300,
        "center_x": 0,
        "top": 300,
        "height": 40
      }
    },
    
    {
      "outlet": "password_field",
      "next_outlet": "confirm_password_field",
      "type": "text_field",
      "password": true,
      "placeholder": "Senha",
      "scroll_top": 0,
      "underlined": true,          
      "autolayout": {
        "width": 140,
        "center_x": -80,
        "top": 350,
        "height": 40
      }
    },  
    
    {
      "outlet": "confirm_password_field",
      "type": "text_field",
      "password": true,
      "placeholder": "Confirme a senha",
      "scroll_top": 0,
      "underlined": true,
      "autolayout": {
        "width": 140,
        "center_x": 80,
        "top": 350,
        "height": 40
      }
    },                
    
    {
      "outlet": "age_button", 
      "type": "button",
      "checkbox": true,
      "action": "toggleUnderAge",
      "corner_radius": 6.0,
      "autolayout": {
        "top": 418,
        "center_x": -140,
        "width": 25,
        "height": 25
      }
    },               
    
    {
      "outlet": "age_label", 
      "type": "label",
      "text_alignment": "left",
      "autolayout": {
        "top": 420,
        "center_x": -35,
        "width": 180,
        "height": 20
      },
      "content": "Tenho menos de 16 anos",
      "font": {
        "name": "Roboto",
        "size": 14,
        "color": "364655"
      }
    },  
    
    {
      "outlet": "accept_terms_button", 
      "type": "button",
      "checkbox": true,
      "action": "toggleAcceptTerms",
      "corner_radius": 6.0,
      "autolayout": {
        "top": 448,
        "center_x": -140,
        "width": 25,
        "height": 25
      }
    },           
    
    {
      "outlet": "accept_terms_label", 
      "type": "label",
      "text_alignment": "left",
      "autolayout": {
        "top": 450,
        "center_x": -35,
        "width": 180,
        "height": 20
      },
      "content": "Li e aceito os",
      "font": {
        "name": "Roboto",
        "size": 14,
        "color": "364655"
      }
    },    
    
    {
      "outlet": "terms_button", 
      "type": "button",
      "action": "view_terms",
      "title": "termos de uso",
      "link_style": true,
      "font": {
        "name": "Roboto",
        "size": 14,
        "color": "364655"
      },
      "autolayout": {
        "top": 450,
        "center_x": 5,
        "width": 100,
        "height": 20
      },
    },
    {
      "outlet": "finish_signup_button", 
      "type": "button",
      "title_color": "ffffff",
      "color": "F15E4F",
      "action": "finish_signup",
      "title": "Criar conta",
      "corner_radius": 6.0,
      "autolayout": {
        "bottom": -15,
        "leading": 20,
        "trailing": -20,
        "height": 50,
        "attach_to_super_super_view": true,
      },
      "font": {
        "name": "Roboto-Bold",
        "size": 20,
        "color": "ffffff"
      }
    }
  ]
}