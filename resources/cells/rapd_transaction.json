{
  "elements": [
    
    {
      "outlet": "avatar",
      "type": "image",
      "corner_radius": 17,
      "autolayout": {
        "center_y": 0,
        "leading": 20,
        "width": 35,
        "height": 35
      }
    },
    {
      "outlet": "name_label",
      "type": "label",
      "autolayout": {
        "leading": 70,
        "trailing": -20,
        "center_y": -10,
        "height": 20
      },
      "font": {
        "name": "Roboto-Bold",
        "size": 17,
        "color": "000000"
      }
    },

    {
      "outlet": "timestamp_label",
      "type": "label",
      "autolayout": {
        "leading": 70,
        "trailing": -20,
        "center_y": 10,
        "height": 20
      },
      "font": {
        "name": "Roboto-Light",
        "size": 11,
        "color": "999999"
      }
    },           
     
    {
      "outlet": "amount_label",
      "type": "label",
      "text_alignment": "right",
      "autolayout": {
        "leading": 80,
        "trailing": -20,
        "center_y": 0,
        "height": 30
      },
      "font": {
        "name": "Roboto",
        "size": 15,
        "color": "333333"
      }
    }            
  ]
}
