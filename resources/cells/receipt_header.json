        "height": 20
      },
      "font": {
        "name": "Roboto",
        "size": 15,
        "color": "9B9B9B"
      }
    },

    {
      "outlet": "usercpf_label", 
      "type": "label",
      "text_alignment": "left",
      "content": "CPF: ",
      "autolayout": {
        "center_y": 20,
        "leading": 85,
        "trailing": 0,
        "height": 20
