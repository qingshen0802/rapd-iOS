{
  "elements": [
    {
      "type": "image",
      "outlet": "avatar",
      "corner_radius": 30,
      "autolayout": {
        "center_y": 0,
        "leading": 15,
        "width": 60,
        "height": 60
      }
    },
    
    {
      "outlet": "name_label", 
      "type": "label",
      "text_alignment": "left",
