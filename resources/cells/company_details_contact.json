{
  "elements": [
    {
      "outlet": "icon",
      "type": "image",
      "name": "images/account_phone_icon.png",
      "autolayout": {
        "leading": 25,
        "center_y":0,
        "width": 25,
        "height": 25
      }
    },

    {
      "outlet": "telephone_button",
      "type": "button",
      "title": "TELEFONE",
      "action": "call_store",
      "text_alignment": "left",
      "autolayout": {
        "leading": -30,
        "width": 230,
        "center_y": -10,
        "height":80
      },
      "font": {
        "name": "Roboto",
        "size": 12,
        "color": "666666"
      }
    },

    {
      "outlet": "telephone_label", 
      "type": "label",
      "text_alignment": "left",
      "autolayout": {
        "top": 30,
        "leading": 55,
        "trailing": -20,
        "height": 20
      },
      "font": {
        "name": "Roboto-bold",
        "size": 20,
        "color": "666666"
      }
    },

    {
      "outlet": "separator",
      "type": "label",
      "background_color": "D3D3D3",
      "autolayout": {
        "top": 0,
        "center_x": 40,
        "width": 1,
        "height": 70
      },
      "content": "",
      "font": {
        "name": "Roboto-Light",
        "size": 14,
        "color": "666666"
      }
    },

    {
      "outlet": "site_icon",
      "name": "images/store_details_site.png",
      "type": "image",
      "corner_radius": 4.0,
      "autolayout": {
        "width": 25,
        "height": 25,
        "center_x": 70,
        "center_y": 0,
      }
    },

    {
      "outlet": "site_button",
      "type": "button",
      "title": "SITE",
      "text_alignment": "left",
      "action": "view_site",
      "autolayout": {
        "center_x": 110,
        "width": 130,
        "center_y": 0,
        "height":60
      },
      "font": {
        "name": "Roboto",
        "size": 14,
        "color": "666666"
      }
    },
  ]
}
